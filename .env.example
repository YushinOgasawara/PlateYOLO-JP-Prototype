# PlateYOLO-JP 環境設定ファイル

# 動画ファイルのベースディレクトリ
VIDEO_BASE_DIR=./videos

# 出力ファイルのベースディレクトリ  
OUTPUT_BASE_DIR=./outputs

# 特定日付のディレクトリ（yyyy-mm-dd形式）
# 例: VIDEO_DATE=2024-01-15 の場合、./videos/2024-01-15/ を参照
VIDEO_DATE=

# デフォルトの動画ディレクトリ（DATE指定がない場合）
DEFAULT_VIDEO_DIR=./videos/

# モデルファイルパス
LPD_MODEL_PATH=weight/PlateYOLO-JP-640x640.onnx
LPR_MODEL_PATH=weight/EkMixer-128x128.onnx

# 推論設定
LPD_SCORE_THRESHOLD=0.3
LPR_MIN_WIDTH1=110
LPR_MIN_WIDTH2=150

# GPU使用設定
USE_GPU=false

# プライバシーモード
USE_PRIVACY_MODE=false